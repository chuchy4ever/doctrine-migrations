{
  "name": "nettrine/migrations",
  "description": "Doctrine Migrations for Nette Framework",
  "keywords": ["nette", "doctrine", "migrations"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/doctrine-migrations",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    },
    {
      "name": "Josef Benjac",
      "homepage": "http://josefbenjac.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "contributte/di": "^0.6.0",
    "doctrine/migrations": "^3.6.0",
    "symfony/console": "^4.2.5|^5.0.0|^6.0.0"
  },
  "require-dev": {
    "doctrine/orm": "^2.6",
    "mockery/mockery": "^1.3.0",
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.3",
    "contributte/phpstan": "^0.1"
  },
  "autoload": {
    "psr-4": {
      "Nettrine\\Migrations\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.10.x-dev"
    }
  }
}
